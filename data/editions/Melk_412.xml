               <msContents>
                  <msItem n="1">
                     <locus from="1" to="16">1-16</locus>
                     <author>Bede</author>
                     <title>De natura rerum</title>
                     <titlePart/>
                  </msItem>
                  <msItem n="2">
                     <locus from="16" to="26">16-26</locus>
                     <author>Bede</author>
                     <title>De temporibus</title>
                     <titlePart/>
                  </msItem>
                  <msItem n="3">
                     <locus from="26" to="27">26-27</locus>
                     <author/>
                     <title>On the planets</title>
                     <incipit>Itaque stella veneris et mercirii hoc a superioribus tribus planetarum
                        stellis differunt...</incipit>
                     <titlePart/>
                     <listBibl>
                        <bibl>Thorndike-Kibre 797.7</bibl>
                     </listBibl>
                  </msItem>
                  <msItem n="4">
                     <locus from="27">27</locus>
                     <author>On the absides of planets</author>
                     <title/>
                     <titlePart>De absidibus planetarum</titlePart>
                     <incipit>Circulus zodiacus quo duodecim signis constat omni es parte aeque
                        vicinus est terrae...</incipit>
                     <explicit>... quod subiecta melius pictura declarabitur</explicit>
                     <note>partly later additions with two rough diagrams</note>
                     <listBibl>
                        <bibl>Thorndike-Kibre 224.14</bibl>
                     </listBibl>
                  </msItem>
                  <msItem n="5">
                     <locus from="28">28</locus>
                     <author/>
                     <title>Easter table</title>
                     <titlePart>Cursus lunae per duodecim signa</titlePart>
                     <note>nahzeitiger Nachtrag</note>
                  </msItem>
               </msContents>
